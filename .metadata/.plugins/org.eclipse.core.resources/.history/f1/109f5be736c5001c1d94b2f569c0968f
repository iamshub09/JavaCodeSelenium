
public class Constructor {
	int id;
	String name;
	
	// parameter less
	public Constructor() {
		System.out.println("This is parameterless constructor");
	}
	
	//parameterlized 
	public Constructor(int id) {
		this.id = id;
		System.out.println(id);
	}
	
	// multiple parameters 
	public Constructor(int stdID, String stdName) {
		id =stdID;
		name = stdName;
		System.out.print(id);
		System.out.println(name);
	}
	
	
}
