


public class UseDifferntClassMethods {

	public static void main(String[] args) {
		// create a object 
		// className refVariable = keyword constructor 
		
		ClassAndObjects s1 = new ClassAndObjects();
		s1.stdID = 101;
		s1.stdName = "Shubham";
		//s1.schoolName ="Lawrence";
		
		
		ClassAndObjects s2 = new ClassAndObjects();
		s2.stdID = 102;
		s2.stdName = "maa baap ki galti";
		//s2.schoolName ="Lawrence";
	
		
		System.out.println(s1.stdName);
		System.out.println(s2.stdName);	
		System.out.println(s1.schoolName);
		System.out.println(s2.schoolName);	
		
		s1.doHomework();
		
		
		
		
		

	}

}
